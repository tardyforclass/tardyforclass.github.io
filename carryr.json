{
  "$jason": {
    "head": {
      "title": "Jasonbase",
      "actions": {
        "visit": [
          {
            "{{#if /\\/edit/.test($jason.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.replace('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
    "body": {
      "header": {
        "menu": {
          {
  "type": "label",
  "text": "Click To Pay With Card",
  "href": {
    "url": "https://pay.google.com/payments/u/0/home#sendRequestMoney",
    "view": "web"
  }
}

        }
      }
      "background": {
        "type": "html",
        "url": "http://vkprakash.github.io/carryr/home.html",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}

{
  "$jason": {
    "head": {
      "title": "Jasonbase",
      "actions": {
        "visit": [
          {
            "{{#if /\\/edit/.test($jason.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.replace('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
    "body": {
	    "header": {
	"menu": {
		
  "items": [{
    "type": "vertical",
    "components": [{
      "type": "button",
      "url": "https://i0.wp.com/www.matuloo.com/wp-content/uploads/2017/02/backbutton.png?fit=800%2C400",
      "action": {
	      "type: "$back"
      } 
    }]
  }]
}
		
},
      
     
	
      
      
      "background": {
        "type": "html",
        "url": "https://vkprakash.github.io/carryr/index.html",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}

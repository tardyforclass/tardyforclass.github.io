{
  "$jason": {
    "head": {
      "title": "Jasonbase",
      "actions": {
        "visit": [
          {
            "{{#if /\\/edit/.test($jason.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.replace('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
    "body": {
      
      "header": {
	"menu": {
		"items": [{
    "type": "label",
    "text": "touch me",
    "action": {
	    "type":"$back"
    }
  }]
		
 
	}
},
      
      
      "background": {
        "type": "html",
        "url": "https://vkprakash.github.io/carryr/index.html",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}

{
  "$jason": {
    "head": {
      "title": "Jasonbase",
      "actions": {
        "visit": [
          {
            "{{#if /\\/edit/.test($jason.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.replace('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
	 
    "body": {
	    "header": {
        "title": "Carryr",
        "style": {
          "background": "#8bb92d",
          "color": "#ffffff",
          "font": "AvenirNext-Bold",
          "size": "14"
        },
      "background": {
        "type": "html",
        "url": "https://vkprakash.github.io/carryr/index.html",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}

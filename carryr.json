{
  "$jason": {
    "head": {
      "title": "Jasonbase",
      "actions": {
        "visit": [
          {
            "{{#if /\\/edit/.test($jason.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.replace('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
    "body": {
      "components": [
         "type": "button",
          "text": "Follow",
              "style": {
                "color": "#ff0000",
                "background": "#000000"
              },
      ],


      
      "background": {
        "type": "html",
        "url": "https://vkprakash.github.io/carryr/index.html",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}

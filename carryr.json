{
  "$jason": {
    "head": {
      "title": "Jasonbase",
      "actions": {
        
        
        "ahref_handler": [{
          "{{#if $jason.url.indexOf('tel') !== -1 }}": {
            "type": "$href",
            "options": {
              "view": "app",
              "url": "{{$jason.url}}"
            }
          }
        }, {
          "{{#else}}": {
            "type": "$default"
          }
        }]
        
        
        
        
        
        
        
        "visit": [
          {
            "{{#if /\\/edit/.test($jason.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.replace('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
    "body": {
       "header": {
       
        "menu": {
          "text": "Pay With Card",
          
        "style": {
          "color": "#44D457",
         
          "font": "AvenirNext-Bold",
          "size": "17"
        },
          "href": {
            "url": "https://pay.google.com/payments/u/0/home#sendRequestMoney",
            "view": "web"
          }
        }
      },
      "background": {
        "type": "html",
        "url": "https://vkprakash.github.io/carryr/",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}

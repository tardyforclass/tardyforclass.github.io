{
  "$jason": {
    "head": {
      "title": "Jasonbase",
      "actions": {
        "visit": [
          {
            "{{#if /\\/edit/.test($jason.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.replace('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
    "body": {
      "layers": [{
        "type": "label",
        "text": "back",
        "style": {
          "top": "10",
          "right": "10",
          "width": "500",
          "height": "500"
        },
        "action": {
          "type": "$back"
        }
      }],
      "background": {
        "type": "html",
        "url": "https://vkprakash.github.io/carryr/index.html",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}
